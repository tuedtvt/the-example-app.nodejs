pipeline {
    
    agent any  
    stages {
        stage('Prepare') {
                    
            steps {
                checkout scm
            }
        }

        stage('Build') {
                    
            steps {
                sh '''
                    echo $PWD
                    chmod +x -R /jenkins/build/build.sh
                    ./jenkins/build/build.sh
                '''
            }
        }
    }
}
